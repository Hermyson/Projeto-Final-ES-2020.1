<p id="notice"><%= notice %></p>

<h1>Psicologos</h1>

<table>
  <thead>
    <tr>
      <th>Crp</th>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @psicologos.each do |psicologo| %>
      <tr>
        <td><%= psicologo.crp %></td>
        <td><%= psicologo.nome %></td>
        <td><%= link_to 'Mostrar', psicologo %></td>
        <td><%= link_to 'Editar', edit_psicologo_path(psicologo) %></td>
        <td><% if can? :destroy,Psicologo %>
            <%= link_to 'Delete', psicologo, method: :delete, data: { confirm: 'Are you sure?' } %>
        <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if can? :create,Psicologo %>
  <%= link_to 'Novo Psicologo', new_psicologo_path %>
<% end %>
<%= link_to 'Voltar',root_path%>